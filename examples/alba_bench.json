{
  "ci": {
    "setup": true,
    "validation": true,
    "cleanup": false,
    "config_manager": "arakoon",
    "user": {
      "shell": {
        "username": "root",
        "password": "rooter"
      },
      "api": {
        "username": "admin",
        "password": "admin"
      }
    },
    "grid_ip": "172.19.5.30"
  },
  "setup": {
    "backends": [
      {
        "name": "mybackend",
        "scaling": "LOCAL",
        "presets": [
          {
            "name": "mypreset",
            "compression": "snappy",
            "encryption": "none",
            "policies": [
              [
                2,2,3,4
              ]
            ],
            "fragment_size": 1
          }
        ],
        "osds": {
          "172.19.5.30": {
            "sdi": 1,
            "sdj": 1,
            "sdk": 1,
            "sdl": 1,
            "sdm": 1,
            "sdn": 1
          }
        }
      }
    ],
    "storagerouters": {
      "172.19.5.30": {
        "hostname": "cmp01",
        "disks": {
          "sda": {
            "roles": [
              "READ",
              "DB",
              "WRITE",
              "SCRUB"
            ]
          },
          "sdh":{
            "roles":[
              "SCRUB"
            ]
          }
        },
        "vpools": {
          "myvpool01": {
            "backend_name": "mybackend",
            "preset": "default",
            "storage_ip": "172.19.5.30",
            "fragment_cache": {
              "strategy": {"cache_on_read":true, "cache_on_write":true},
              "location": "disk",
              "settings": null
            },
            "storagedriver": {
              "sco_size": 4,
              "cluster_size": 4,
              "volume_write_buffer": 512,
              "strategy": "none",
              "global_write_buffer": 7,
              "global_read_buffer": 0,
              "deduplication": "non_dedupe",
              "dtl_transport": "tcp",
              "dtl_mode": "a_sync"
            }
          },
          "myvpool02": {
            "backend_name": "mybackend",
            "preset": "default",
            "storage_ip": "172.19.5.30",
            "fragment_cache": {
              "strategy": {"cache_on_read":true, "cache_on_write":true},
              "location": "disk",
              "settings": null
            },
            "storagedriver": {
              "sco_size": 4,
              "cluster_size": 4,
              "volume_write_buffer": 512,
              "strategy": "none",
              "global_write_buffer": 7,
              "global_read_buffer": 0,
              "deduplication": "non_dedupe",
              "dtl_transport": "tcp",
              "dtl_mode": "a_sync"
            }
          }
        }
      },
      "172.19.5.33": {
        "hostname": "cmp04",
        "disks": {
          "sdd": {
            "roles": [
              "READ",
              "DB",
              "WRITE",
              "SCRUB"
            ]
          }
        }
      }
    }
  }
}