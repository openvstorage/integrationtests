              [
                1,2,2,3
              ]
            ],
            "fragment_size": 1
          }
        ],
        "osds": {
          "mybackend": "mypreset",
          "mybackend02": "mypreset"
        }
      }
    ],
    "storagerouters": {
      "10.100.199.161": {
        "hostname": "ovs-node-1",
        "domains": {"domain_guids": ["Gravelines"], "recovery_domain_guids": ["Roubaix", "Strasbourg"]},
        "disks": {
          "sde": {
            "roles": [
