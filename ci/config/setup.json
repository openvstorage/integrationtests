            "backend_name": "mybackend-global",
            "preset": "mypreset",
            "storage_ip": "10.100.199.161",
            "fragment_cache": {
              "strategy": {"cache_on_read":true, "cache_on_write":true},
              "location": "disk"
            },
            "storagedriver": {
              "sco_size": 4,
              "cluster_size": 4,
              "volume_write_buffer": 512,
              "strategy": "none",
              "global_write_buffer": 7,
              "global_read_buffer": 0,
              "deduplication": "non_dedupe",
              "dtl_transport": "tcp",
